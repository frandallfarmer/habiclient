; farebox_chime:
;		Uses PW ramps
	byte	0x2d		;Gate Ramp ADSR Wave 
	byte	0x0,0x10,0x28,0x0,0x0,0x0	; Start_Freq,Duration,Increment
	byte	0x9,0x0	; ADSR
	byte	0x40	; Waveform

	byte	0x20		;Wave 
	byte	0x0	; Waveform

	byte	0x2		;Stop 
	byte	0x0	; next_sfx_number
; farebox_chime.pw:
	; Pulsewidth Ramp starts here
	byte	0x4		;Ramp 
	byte	0x0,0x4,0x10,0x1,0x40,0x0	; Start_Freq,Duration,Increment

	byte	0x2		;Stop 
	byte	0x0	; next_sfx_number


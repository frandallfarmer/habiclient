	byte	0x0,0x0,0x10,0x1,0x80,0x2	; Start_Freq,Duration,Increment

	byte	0x20		;Wave 
	byte	0x0	; Waveform

	byte	0x2		;Stop 
	byte	0x0	; next_sfx_number

